{
    "name": "Agenticone",
    "type": "project",
    "description": "Agenticone CRM",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-dompdf": "^2.0.0",
        "diglactic/laravel-breadcrumbs": "^8.0",
        "doctrine/dbal": "^3.0",
        "enshrined/svg-sanitize": "^0.21.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "khaled.alshamaa/ar-php": "^6.3",
        "konekt/concord": "^1.10",
        "krayin/rest-api": "^2.1",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^4.5",
        "maatwebsite/excel": "^3.1",
        "mpdf/mpdf": "^8.2",
        "prettus/l5-repository": "^2.7.9",
        "smalot/pdfparser": "^2.11",
        "webklex/laravel-imap": "^5.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "krayin/krayin-package-generator": "dev-master",
        "laravel/pint": "^1.16",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.6",
        "pestphp/pest-plugin-laravel": "^2.1",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "agenticone\\Activity\\": "packages/agenticone/Activity/src",
            "agenticone\\Admin\\": "packages/agenticone/Admin/src",
            "agenticone\\Attribute\\": "packages/agenticone/Attribute/src",
            "agenticone\\Contact\\": "packages/agenticone/Contact/src",
            "agenticone\\Core\\": "packages/agenticone/Core/src",
            "agenticone\\DataGrid\\": "packages/agenticone/DataGrid/src",
            "agenticone\\DataTransfer\\": "packages/agenticone/DataTransfer/src",
            "agenticone\\Email\\": "packages/agenticone/Email/src",
            "agenticone\\EmailTemplate\\": "packages/agenticone/EmailTemplate/src",
            "agenticone\\Marketing\\": "packages/agenticone/Marketing/src",
            "agenticone\\Installer\\": "packages/agenticone/Installer/src",
            "agenticone\\Lead\\": "packages/agenticone/Lead/src",
            "agenticone\\Product\\": "packages/agenticone/Product/src",
            "agenticone\\Quote\\": "packages/agenticone/Quote/src",
            "agenticone\\Tag\\": "packages/agenticone/Tag/src",
            "agenticone\\User\\": "packages/agenticone/User/src",
            "agenticone\\Warehouse\\": "packages/agenticone/Warehouse/src",
            "agenticone\\WebForm\\": "packages/agenticone/WebForm/src",
            "agenticone\\Automation\\": "packages/agenticone/Automation/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/*/*",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
